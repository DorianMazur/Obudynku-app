{
  "name": "ob-backend",
  "version": "0.0.0",
  "author": "Dorian Mazur <mazur.dorian15@gmail.com>",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
